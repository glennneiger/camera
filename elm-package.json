{
    "version": "1.0.0",
    "summary": "Render 3D OpenSolid types using WebGL",
    "repository": "https://github.com/opensolid/webgl.git",
    "license": "MPL-2.0",
    "source-directories": [
        "src"
    ],
    "exposed-modules": [
        "OpenSolid.WebGL.Color",
        "OpenSolid.WebGL.Vector2d",
        "OpenSolid.WebGL.Vector3d",
