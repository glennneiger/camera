    "dependencies": {
        "elm-community/linear-algebra": "3.0.0 <= v < 4.0.0",
        "elm-community/webgl": "2.0.3 <= v < 3.0.0",
        "elm-lang/core": "5.1.1 <= v < 6.0.0",
        "elm-lang/html": "2.0.0 <= v < 3.0.0",
        "opensolid/geometry": "1.1.0 <= v < 2.0.0"
